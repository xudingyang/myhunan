.success {
  background: #fff;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.success .icon {
  margin-top: 60rpx;
}

.success .text {
  margin-top: 30rpx;
  font-size: 34rpx;
}
.text2 {
  margin-top: 30rpx;
  font-size: 30rpx;
  color: #888;
}
.success-btn {
  box-sizing: border-box;
  margin-top: 20rpx;
  height: 160rpx;
  display: flex;
  align-items: center;
}

.back-txt {
  background: #19c017;
  color: #fff;
  font-size: 36rpx;
  border-radius: 10rpx;
  width: 500rpx;
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  letter-spacing: 2px;
}