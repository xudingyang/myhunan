page {
  background: #f5f5f5;
}
.wrapper {
  padding: 30rpx 0;
}
.item {
  padding-left: 20rpx;
  background: #fff;
  color: #838383;
  font-size: 30rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
}
.item1 > text {
  margin-left: 6px;
  color: #333;
}
.item2 {
  display: flex;
  align-items: center;
}
.item2 > text {
  margin-left: 20px;
  display: inline-block;
  font-size: 32rpx;
  height: 50rpx;
  width: 90rpx;
  line-height: 50rpx;
  text-align: center;
  border: 1px solid #aaa;
  border-radius: 6rpx;
  background: #eee;
}